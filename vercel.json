{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@now/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ],
    "build": {
    "includeFiles": ["node_modules/mongodb/**", "node_modules/mongoose/**"]
    }
  }